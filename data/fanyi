    function checkQuota() {
        var r = 190596091 / 5368709120;
        if (r >= 1) {
            UI.alert("您的邮箱容量已满, 请清理不需要的邮件");
        } else if (r >= 0.9) {
            UI.alert("您的邮箱容量将满, 请清理不需要的邮件");
        }
    }
    Event.observe(window, "load", (checkQuota));

