function popCreatWin(){
    new UI.Dialog({
        title  : "新建文件夹",
        body   :
                 '  <table width="410px" height="60"> <tbody><tr><td width="120" align="right">输入文件夹名称:</td><td>'+
                 '  <input type="text" id="foldername" class="input createfolder" value="' + newFolderName + '"/></td>'+
                 '  <tr><td align="right" >新建文件夹位置:</td><td>'+folderSelectHTML+'</td></tr>'+
                 '  </tbody></table>',
        button : "OK_CANCEL",
        action : function(sForm) {
            if (validateName($("foldername").value)) {
                new CMXClient().simpleCall("mbox:createUserFolders", {
                    items: [
                        {
                            parent: $("folder-parent").value - 0,
                            name: $("foldername").value
                        }
                    ]
                }, function () {
                    if (MM) {
                        CC.folderInfo = {}; // reset folder info
                        MM.invalidModule(["folder", "search","letter_"]);
                    }
                    reloadNav();
                    UI.alert("新建文件夹成功");
                });
            }
        },
        cache  : true
    })
}
    window.getPOPfolderSelectHTML = getPOPfolderSelectHTML;
    window.popCreatWin = popCreatWin;
    window.autoCreateFolderName = autoCreateFolderName;
})();
    
    
