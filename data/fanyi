<script type="text/javascript"><!--

/* 导航选项鼠标经过状态 for IE6 */
//if (jQ.browser["msie"] && jQ.browser.version == "6.0") {
    jQ("#navContainer").bind("mouseover", function(event) {
        var element = Event.findElement(event || window.event, "LI");
        if (element.tagName && element.tagName.toUpperCase() == "LI") {
            Element.addClassName(element, "hover");
        } else {
            element = Event.findElementByClassName(event || window.event, "cexpand");
            if (element.className !== undefined && Element.hasClassName(element, "cexpand")) {
                Element.addClassName(element, "hover");
            }
        }
    });
    jQ("#navContainer").bind("mouseout", function(event) {
        var element = Event.findElement(event || window.event, "LI");
        if (element.tagName && element.tagName.toUpperCase() == "LI") {
            Element.removeClassName(element, "hover");
        } else {
            element = Event.findElementByClassName(event || window.event, "cexpand");
            if (element.className !== undefined && Element.hasClassName(element, "cexpand")) {
                Element.removeClassName(element, "hover");
            }
        }
    });
//}
function setOverflow(){
    var viewHeight=document.body.clientHeight;

    var box =$('adjustSizeBox');
    var box_positionY=getY(box);
    var boxHeight=viewHeight-box_positionY;

    var spans=document.getElementsByTagName("span");
    var emptyFolders = document.getElementsByClassName("nav_empty");
    var len=spans.length;
    var getPop = document.getElementsByClassName("nav_get_pop");

    if(box.clientHeight>boxHeight || box.scrollHeight>boxHeight){
        box.style.height=boxHeight+"px";
        Element.addClassName(box,"navDivOverflow");
        for(var i=0;i<len;i++){
            if(spans[i].getAttribute("name")=="expandLi")
                spans[i].style.width="140px";
        }
        for (var j = 0; j < emptyFolders.length; j++) {
            jQ(emptyFolders[j]).prev("a").css("width", "120px");
        }
        for (var k = 0; k < getPop.length; k++) {
            jQ(getPop[k]).css("margin-left", "5px");
        }
        jQ('a[id*=navCal]').css('width','130px');
    }else{
        box.style.height="auto";
        Element.removeClassName(box,"navDivOverflow");
        for(i = 0; i < len; i++){
            if(spans[i].getAttribute("name")=="expandLi")
                spans[i].style.width="150px";
        }
        for (j = 0; j < emptyFolders.length; j++) {
            jQ(emptyFolders[j]).prev("a").css("width", "140px");
        }
        for (k = 0; k < getPop.length; k++) {
            jQ(getPop[k]).css("margin-left", "16px");
        }
        jQ('a[id*=navCal]').css('width','140px');
    }
    function getY(e) {
        // 当前element的offsetTop可能以body作为参考系，也可能以父elemnet作为参考系，具体看浏览器类型
        var t = e.offsetTop;
        while (e = e.offsetParent) {
            t += e.offsetTop;
        }
        return t;
    }

}
(function () {
    var selectedObjId;
    var expandedObjIds = {};

    function jsHandleExpand (nod, expanded) {
        if (nod.parentNode.tagName.toUpperCase() == "LI") {
            // <li><i>[nod]</i><a id="navFid_1">[name]</a>
            var anchorElement = nod.nextSibling;
            if (anchorElement && anchorElement.id) {
                if (expanded) {
                    expandedObjIds[anchorElement.id] = true;
                } else {
                    delete expandedObjIds[anchorElement.id];
                }

            }
        }
        setOverflow();
    }

    /*导航选项选中状态*/
    function setSelect(obj) {
        if (selectedObjId > "" && $(selectedObjId)) {
            $(selectedObjId).parentNode.className = "";
            Element.removeClassName($(selectedObjId), "active");
        }
        if (obj && $(obj)) {
            if ($(obj).getAttribute("ignoreactive") != "true") {
                Element.addClassName($(obj), "active");
                if (!$(obj).href) {
                    if (jQ(obj).children("i").attr("class") === 'tgl COLLAPSED'
                            || jQ(obj).children("i").attr("class") === 'tgl FCLOSE') {
                        Element.removeClassName($(obj), "active");
                    }
                }
                $(obj).parentNode.className = "active";
            }
            selectedObjId = $(obj).id;
        } else {
            selectedObjId = null;
        }
    }
    function setSelectAndVisible(obj) {
        if (obj) {
            expandAllParents(obj, false);
        }
        setSelect(obj);
    }

    function restoreLastState() {
        for (var objId in expandedObjIds) {
            if ($(objId)) {
                // <li><i>[nod]</i><a id="navFid_1">[name]</a>
                tGL_show($(objId).previousSibling);
            }
        }
        
        if (selectedObjId > "") {
            setSelectAndVisible(selectedObjId);
        }
    }

    function expandAllParents(objIdForFolder, expandThisNode) {
        if (objIdForFolder.indexOf("navFid_") != 0 || !$(objIdForFolder)) {
            return;
        }

        if (expandThisNode) {
            tGL_show($(objIdForFolder).previousSibling);
        }

        var parentUL = $(objIdForFolder).parentNode.parentNode;
        if (parentUL.id == "nav_other" || parentUL.id == "nav_pop") {
            clicknode_show(parentUL.id + "_nod", parentUL);

        } else if (parentUL.previousSibling) {
            var parentObj = parentUL.previousSibling;
            if (parentObj.id == null || parentObj.id.indexOf("navFid_") != 0) {
                // 包含清空时
                // <li><i>[nod11]</i><a id="navFid_11">[name11]</a><a class="nav_empty">..</a>
                //     ...
                // </li>
                parentObj = parentUL.parentNode.childNodes[1];
            }
            var parentObjId = parentObj.id;
            if (parentObjId) {
                expandAllParents(parentObjId, true);
            }
        }
    }


    window.jsHandleExpand         = jsHandleExpand;
    window.setSelect              = setSelect;
    window.restoreLastState       = restoreLastState;
    window.setSelectMain          = function(mainPath, fid, selectedObj) {
        var objId = selectedObj;
        if (objId) {
            // skip
        } else if (mainPath) {
            if (mainPath.indexOf("/mbox/") == 0) {
                objId = fid ? ("navFid_" + fid) : null;
            } else if (mainPath.indexOf("/pab/savercpt") == 0) {
             // skip
            } else if (mainPath.indexOf("/pab/") == 0) {
                objId = "navPab";
            } else if (mainPath.indexOf("/oab/") == 0) {
                objId = "navOab";
            } else if (mainPath.indexOf("/cal/") == 0) {
                objId = fid ? ("navCal_" + fid) : null;
            } else if (mainPath.indexOf("/nf/") == 0) {
            
                objId = (fid == 9) || (mainPath.indexOf("/nf/trs") == 0)
                        ? "navBnf" : "navNf";
            } else if (mainPath.indexOf("/subs/") == 0) {
                objId = "navSubs";
            }
        }
        setSelectAndVisible(objId);
    };

    jQ.event.add(window, "resize", setOverflow);
    jQ(document).ready(function() {
        setOverflow();
    });
})();
function clearOrGetPOPFolder (s, emptyURL, postData) {
    postData = postData || "";
    var redirectURL = emptyURL;
    UI.confirm({
        message: s,
        yes: function() {
            if (MM) {
                MM.getModule(redirectURL, "folder", false, postData);
            } else {
                $("main").src = redirectURL + "&" + postData;
            }
        }
    });
}
