<script type="text/javascript">
    function displayDetailDate() {
        if (document.getElementById("filter.sentDateString").value == "display_detail") {
            if (Element.visible('displayDetailDate')) {
                document.getElementById("filter.sentDate").value = "";
            } else {
                document.getElementById("filter.detailDateBegin").value = "";
                document.getElementById("filter.detailDateEnd").value = "";
            }
            Element.toggle('displayDetailDate');
        } else if (Element.visible('displayDetailDate')) {
            Element.toggle('displayDetailDate');
            document.getElementById("filter.sentDate").value = "";
        }
    }
    function checkSearchForm(form) {
        var sendDateString = document.getElementsByName("filter.sentDateString")[0].value;
        var receiveDate = document.getElementsByName("filter.receivedDate")[0].value;
        var pattern = document.getElementById("pattern");
        var fid = document.getElementById("fid");
        var mboxa = document.getElementById("mboxa");
        if (fid.value.indexOf(":") > -1) { //特殊处理归档搜索
            mboxa.value = fid.value.split(":")[0];
            fid.value = fid.value.split(":")[1];
        }
        var vid = document.getElementById("vid");
        if (vid != null && vid.value != -1) {
            mboxa.value = "@" + vid.value;
            jQ("#vid > option").each(function() {
                if (this.value == vid.value) {
                    jQ("#account").val(this.text);
                }
            });
        }

        var sendDate = null;
        var detailDateBegin = document.getElementById("filter.detailDateBegin").value;
        var detailDateEnd = document.getElementById("filter.detailDateEnd").value;
        if (sendDateString != "" && sendDateString != "display_detail") {
            var sendDateArr = sendDateString.split("_");
            var sendDateType = sendDateArr[0];
            var sendDateNumber = sendDateArr[1];
            sendDate = new Date();
            if (sendDateType == "day"){
                sendDate.setYear(sendDate.getFullYear());
                sendDate.setMonth(sendDate.getMonth());
                sendDate.setDate(sendDate.getDate() - sendDateNumber + 1);
            } else if (sendDateType == "month") {
                sendDate.setYear(sendDate.getFullYear());
                sendDate.setMonth(sendDate.getMonth() - sendDateNumber);
                sendDate.setDate(sendDate.getDate());
            } else if (sendDateType == "year"){
                sendDate.setYear(sendDate.getFullYear() - sendDateNumber);
                sendDate.setMonth(sendDate.getMonth());
                sendDate.setDate(sendDate.getDate());
            }
            var dayString = sendDate.getDate() < 10 ? ("0" + sendDate.getDate()) : sendDate.getDate();
            var monthString = sendDate.getMonth() + 1;
            sendDateString = sendDate.getFullYear() + "-" + monthString + "-" + dayString;
            document.getElementById("filter.sentDate").value = sendDateString;
        }
        if (detailDateBegin != "" && !Date.isDateFormat(detailDateBegin)) {
            UI.alert(gLang.option.msg["errSendDate"]);
            return false;
        } else if (receiveDate != "" && !Date.isDateFormat(receiveDate)) {
            UI.alert(gLang.option.msg["errReceiveDate"]);
            return false;
        }
        if (detailDateEnd != "") {
            if (!Date.isDateFormat(detailDateEnd)) {
                UI.alert(gLang.option.msg["errSendDate"]);
                return false;
            }
            document.getElementById("filter.detailDateEnd").value = moveDateTime(detailDateEnd, 1);
        }


    
        var size = document.getElementsByName("filter.size")[0].value;
        var opSize = document.getElementById("op.sizeStr").value;
        if (opSize == "mail_size_3") {
            document.getElementById("op.size").value = ">";
            document.getElementById("filter.size").value = 10 * 1024;
        } else if (opSize == "mail_size_2") {
            document.getElementById("op.size").value = "between";
            document.getElementById("filter.size").value = 100;
            document.getElementById("filter.size_end").value = 10 * 1024;
        } else if (opSize == "mail_size_1") {
            document.getElementById("op.size").value = "between";
            document.getElementById("filter.size").value = 0;
            document.getElementById("filter.size_end").value = 100;
        }

        if (size != "" && (parseInt(size) != size || parseInt(size) < 0)) {
            UI.alert(gLang.option.msg["errSearchMailSize"]);
            return false;
        } else if (size != "" && parseInt(size) > 2147483647) { ////查找邮件大小不超过int类型的最大值
            UI.alert(gLang.option.msg["errOverrunMaxSize"]);
            return false;
        }
    

        var inputs = getHintInputs();

        for (var i = 0, len = inputs.length; i < len; i++) {
            if (isByHint(inputs[i])) {
                inputs[i].value = "";
            }
        }

        if (!pattern || !pattern.value) {
            // 不支持全文搜索或没有输入关键字，则默认按时间(发送时间或接收时间，由用户的邮箱设置决定)进行降序排序
            $("order").value = "receivedDate";
        }

        return true;
    }

    function moveDateTime(dateStr, moveDays) {
        dateStr.replace(/:/g, "-");
        dateStr = dateStr.replace(" ", "-");
        dateStr = dateStr.replace(".", "-");
        var dateArray = dateStr.split('-');//yyyy,[MM|M],[dd|d]
        var detailDate = new Date(dateArray[0], --dateArray[1], dateArray[2]);//--dateArray[1] : 从0月到11月，需要纠正
        detailDate.setDate(detailDate.getDate() + moveDays);
        var dayString1 = detailDate.getDate() < 10 ? ("0" + detailDate.getDate()) : detailDate.getDate();
        var monthString1 = detailDate.getMonth() + 1;
        monthString1 = monthString1 < 10 ? ("0" + monthString1) : monthString1;
        return detailDate.getFullYear() + "-" + monthString1 + "-" + dayString1;
    }


    function getHintInputs() {
        return document.getElementsByClassName("hasHint", "mboxSearch");
    }

    function markHint(el) {
        el.value = el.getAttribute("hint");
        Element.addClassName(el, "fHint");
    }

    function clearHint(el) {
        el.value = '';
        Element.removeClassName(el, "fHint");
    }

    function isByHint(el) {
        return el.value == el.getAttribute("hint") && Element.hasClassName(el, "fHint");
    }
    function checkFid(fid){
        if(fid == 0){
            $('recursive').checked = true;
            $('recursive').disabled = true;
        }else{
            $('recursive').disabled = false;
        }
    }
    function buildFidSelectObj(vid) {
        jQuery("#vid option").each(function() {
            var value = $(this).value;
            $("fid" + (value != -1 ? "_" + value : "")).style.display = (value != vid ? 'none' : '');
            $("fid" + (value != -1 ? "_" + value : "")).disabled = value != vid;
        });
    }

</script>
<script type="text/javascript"><!--
// 实现logo等比例缩放
function adjustLogo(ImgD, FitWidth, FitHeight)
{
    var width = ImgD.width;
    var height = ImgD.height;
    if (width > 0 && height > 0) {
        if (width / height >= FitWidth / FitHeight) {
            if (width > FitWidth) {
                ImgD.width = FitWidth;
                ImgD.height = (height * FitWidth) / width;
            }
        } else {
            if (height > FitHeight) {
                ImgD.height = FitHeight;
                ImgD.width = (width * FitHeight) / height;
            }
        }
    }
}
jQ(document).ready(function() {
    
    
    
    $("lockscreen").onclick = function(e) {
        var failCount = 0;
        var sessionInvalid = false;
        Event.stop(e || window.event);
        new UI.Dialog({
            title : "锁屏提示",
            body : '<table width="100%">'+
                    '<tbody> '+
                    '<tr> ' +
                    '<td width="50" valign="top"><i class="bico brig alert"></i></td>'+
                    '<td style="word-break:keep-all; white-space:normal;word-wrap:break-word;">'+
                    '<div class="lock-screen-detail"><p>您的邮箱将进入锁定状态</p><ul><li>邮箱将仍然保持在线</li><li>在输入正确的“邮箱密码”前任何人都无法动您的邮箱</li><li>可以更安全的保护您的隐私</li></ul></div>' +
                    '</td>'+
                    '</tr>' +
                    '</tbody>'+
                    '</table>' ,
            button : "OK_CANCEL",
            textOK : "确定锁屏",
            actions : [function(sForm) {
                new CMXClient().simpleCall("user:lockScreen", {}, function(result) {
                    var dialog = new UI.Dialog({
                        title  : "邮箱屏幕解锁",
                        style : "LOCK",
                        allowRefresh : true,
                        body   : ''
                                + ' <table class="std_lock">'
                                + '   <tbody>'
                                + '      <tr>'
                                + '          <th>您的邮箱进入锁屏状态</th>'
                                + '      <tr>'
                                + '          <td class="input_tips"><div style="height:20px;"></div>请输入邮箱密码解锁:</td>'
                                + '      </tr>'
                                + '     <tr>'
                                + '         <td class="input_tips">'
                                + '         <input type="password" id="password_unlock" class="input" value=""/></td>'
                                + '     </tr>'
                                + '     <tr>'
                                + '         <td  class="input_tips"><span id="error_unlock" class="error"></span></td>'
                                + '     </tr>'
                                + '   </tbody>'
                                + ' </table>',
                        button : "OK",
                        action : function(sForm) {
                            if ($win().$("password_unlock").value) {
                                if (sessionInvalid == false) {
                                    var client = new CMXClient();
                                    client.resultListener = function(result) {
                                        if (result.code == "FA_UNAUTHORIZED") {
                                            failCount++;
                                            $win().$("error_unlock").innerHTML = '密码错误,您还有' + (3-failCount)
                                                    + ' 次输入机会!';
                                            $win().$("password_unlock").value = "";
                                            $win().$("password_unlock").focus();
                                            return true;
                                        } else if (result.code == "FA_INVALID_SESSION") {
                                            sessionInvalid = true;
                                        }
                                    };
                                    client.simpleCall("user:unlockScreen", {
                                        password: $win().$("password_unlock").value
                                    }, function (result) {
                                        dialog.hide();
                                    });
                                } else {
                                    top.location = "/coremail/logout.jsp";
                                }
                            } else {
                                $win().$("error_unlock").innerHTML = '密码不能为空';
                                $win().$("password_unlock").focus();
                            }
                            return false;
                        },
                        showMask : true,
                        maskValue : 1,
                        showClose : false,
                        cache  : true
                    });
                });
            }]
        });
    };
    
    function adjustTips() {
        jQ('#browser_tips').css('left', function() {
            return (jQ(document).width() - jQ(this).width()) / 2;
        });
    }
    jQ(window).bind('resize', function() {
        adjustTips();
    });
    jQ('#browser_tips_close').click(function() {
        jQ('#browser_tips').fadeOut('slow');
    });
    adjustTips();

    jQ("#options").bind({
        click:function(){
//            jQ("#headChangeFaces").css("top", jQ("#settingBar").offset().top + 35);
            jQ("#headChangeFaces").css("left", jQ("#options").offset().left);
            jQ("#headChangeFaces").show();                //每次触发显示弹窗都要重新绑定时间到新的tab

            // 当在document其他地方单击时，隐藏弹窗
            jQ(document.getElementById(GE.curModule).contentWindow.document).bind("click", function (e) {
                disaplayOptions(e);
            });
            jQ(document.getElementById(GE.curModule).contentWindow.document.getElementsByTagName("a")).bind("click", function (e) {
                disaplayOptions(e);
            });
            jQ(document.getElementsByTagName("a")).bind("click", function (e) {
                disaplayOptions(e);
            });
            if(document.getElementById(GE.curModule).contentWindow.document.getElementById("htmleditor")) {
                jQ(document.getElementById(GE.curModule).contentWindow.document.getElementById("htmleditor").contentWindow.document).bind("click", function (e) {
                    disaplayOptions(e);
                });
                jQ(document.getElementById(GE.curModule).contentWindow.document.getElementById("htmleditor").contentWindow.document.getElementById("HtmlEditor").contentWindow.document).bind("click", function (e) {
                    disaplayOptions(e);
                });
            }
        }
    });
    jQ(document).bind("click", function (e) {
        disaplayOptions(e);
    });
    function disaplayOptions (e) {
        var target = jQ(e.target);
        if (target.closest("#headChangeFaces").length == 0 && target.closest("#options").length == 0) {
            jQ("#headChangeFaces").css("display","none");
        }
    }

    jQ("span#faceswitch").bind("mouseleave", function(e) {
        jQ("span#faceswitch").hide();
    });
});
//--></script>
