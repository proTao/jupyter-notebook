<script type="text/javascript"><!--

function resizeTabWidth(topWidth) {
    var tabRightWidth = 210;
    tabRightWidth = ($("gSearchForm") || {}).offsetWidth || 0;
    UI.Tab.size(topWidth - tabRightWidth - 25);
}
var doResize = function() {
    var viewport = Dom.getViewPort();

    var topWidth = viewport.width;
    var topHeight = viewport.height;



    var mainHeight = Math.max(0, (topHeight - document.getElementById("headContainer").offsetHeight));
    document.getElementById("mainContainer").style.height = mainHeight + "px";
    document.getElementById("navContainer").style.height = mainHeight + "px";
    
    resizeTabWidth(topWidth);
    
    jQ("div iframe", "#mainContainer").each(function(){
        $(this).style.height = mainHeight + "px";
    });

};



    window.onresize = doResize;
    doResize();

    
        GE_Init();
        initModulePage();

        function initModulePage() {
            
            

            var welcomeURL = "main/welcome.jsp?sid=" + CC.sid;
            var folderURL = "mbox/list.jsp?sid=" + CC.sid + "&fid=1";

            MM.getModule(welcomeURL, null, null, null, {putHistory: false});
            
            MM.getModule(folderURL, null, null, null, {putHistory: false});
            

            var mainPageUrl = "main/welcome.jsp?sid=PAoyKgaafJqNUxgRBIaaZelNmzedHdRI";
            if(mainPageUrl.indexOf("personal.jsp") > -1){
                setTimeout(function(){
                    MM.getModule(mainPageUrl);
                }, 100);
            }else{
                MM.getModule(mainPageUrl);
            }

            var topWidthForTab = Dom.getViewPort().width;
            
            resizeTabWidth(topWidthForTab);
        }
    

