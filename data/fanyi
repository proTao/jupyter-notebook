        <form class="searcharea" id="gSearchForm" method="POST" forwardtotab="search" action="/coremail/XT3/mbox/search.jsp?action=search&amp;sid=PAoyKgaafJqNUxgRBIaaZelNmzedHdRI&amp;defaultInitDateDesc=true">
            <input name="pattern" id="pattern" class="sinput fLeft searchHint" autocomplete="off" value="邮件全文搜索 " type="text">

            <div class="shbtn fLeft">
                <a id="advancedSearch" class="arr fLeft" title="高级搜索" href="#" onclick="initSearch();">
                    <!--[if IE 6]>&nbsp;&nbsp;&nbsp;&nbsp;<![endif]-->
                </a>
            </div>
            <div class="sbtn fLeft" id="searchbtn" title="&nbsp;搜索&nbsp;&nbsp;" onclick="$('gSearchForm').trySubmit()">
            </div>
                
            <script type="text/javascript">
                Object.extend(String.prototype, {
                    trim : function() {
                        return this.replace(/(^\s*)|(\s*$)/g, "");
                    }
                });
                (function() {
                    function getPatternInput() {
                        return Form.getInputs('gSearchForm', "text", "pattern")[0];
                    }

                    $('gSearchForm').trySubmit = function() {
                        if (this.submitSearchForm()) {
                            if (MM) {
                                var forwardToTabName = jQ(this).attr("forwardtotab");
                                var forwardToURL = jQ(this).attr("action");
                                MM.getModule(null, "search");
                                var targetForm = $(this);
                                targetForm.target = GE.getModuleRNDIframeName(GE.getModuleNameById(forwardToTabName));
                                targetForm.submit();
                                return false;
                            } else {
                                this.submit();
                            }
                        }
                        return false;
                    };
                    $('gSearchForm').submitSearchForm = function() {
                        if (hasNothingToSearch()) {
                            parent.UI.alert({
                                message:"请输入要搜索的字或词",
                                callback:function() {
                                    getPatternInput().focus();
                                }
                            });
                            return false;
                        }
                        return true;
                    };
                    $('gSearchForm').onsubmit = function() {
                        return this.trySubmit();
                    };

                    function hasNothingToSearch() {
                        return !getPatternInput().value.trim() || isByHint(getPatternInput());
                    }

                    function markHint(el) {
                        el.value = el.defaultValue;
                        el.className = "sinput fLeft searchHint";
                    }

                    function clearHint(el) {
                        el.value = '';
                        el.className = "sinput fLeft";
                    }

                    function isByHint(el) {
                        return el.value == el.defaultValue && Element.hasClassName(el, "searchHint");
                    }

                    Object.extend(getPatternInput(), {
                        onfocus : function() {
                            if (isByHint(this)) {
                                clearHint(this);
                            }
                        },
                        onblur : function() {
                            if (this.value == '') {
                                markHint(this);
                            }
                        }
                    });

                    if (jQ.browser.msie && jQ.browser.version == "6.0") {
                        jQ("#searchbtn").bind("mouseover", function(){
                            if (Element.hasClassName(this, "sbtn")) {
                                Element.removeClassName(this, "sbtn");
                            }
                            if (!Element.hasClassName(this, "sbtnHover")) {
                                Element.addClassName(this, "sbtnHover");
                            }
                        });
                        jQ("#searchbtn").bind("mouseout", function(){
                            if (Element.hasClassName(this, "sbtnHover")) {
                                Element.removeClassName(this, "sbtnHover");
                            }
                            if (!Element.hasClassName(this, "sbtn")) {
                                Element.addClassName(this, "sbtn");
                            }
                        });
                    }
                })();

                var initializing =false;
                function initSearch () {
                    if (initializing) return;
                    initializing = true;
                    new Ajax.Request('/coremail/XT3/mbox/searchWin.jsp?sid=' + 'PAoyKgaafJqNUxgRBIaaZelNmzedHdRI' , {onSuccess: function(transport) {
                        try {
                            var options = {
                                style : "WIN",
                                body : transport.responseText,
                                init : function(div) {
                                    //初始化日期控件
                                    DatePicker.initDatePickers(div);
                                    var inputs = getHintInputs();
                                    for (var i = 0, len = inputs.length; i < len; i++) {
                                        var input = inputs[i];
                                        var hint = input.getAttribute("hint");
                                        if (hint) {
                                            if (!input.value) {
                                                markHint(input);
                                            }
                                            Object.extend(input, {
                                                onfocus : function() {
                                                    if (isByHint(this)) {
                                                        clearHint(this);
                                                    }
                                                },
                                                onblur : function() {
                                                    if (this.value == '') {
                                                        markHint(this);
                                                    }
                                                }
                                            });
                                        }
                                    }
                                },
                                button  : "Search_CANCEL",
                                actions : [
                                    function() {
                                        var form = document.getElementById("mboxSearch");
                                       if (checkSearchForm(form)) {
                                           if (MM) {
                                               MM.getModule(null,"search", true);
                                               var targetName =  document.getElementById("search").name;
                                               form.target = targetName ;
                                           }
                                           form.submit();
                                       }
                                    },
                                    null
                                ],
                                onClosed: function() {
                                }
                            };
                            dialog = new UI.Dialog(options);
                            var div = jQ("#dvDialogStyle");
                            fixMboxSearchHeight();
                            div.addClass('dvSearch'); //CM-14050 需要定制的dialog样式
                            var span = jQ("#advancedSearch");
                            var left = span.offset().left + span.width() - 374;
                            var top = span.offset().top + span.height() + 5;
                            div.attr("style", "position: absolute; z-index: 999; top: " + top + "px; left:" + left + "px;");
                            div.hide();
                            div.fadeIn();
                            var doc = (document.all ? jQ(window.frames["overWin"].document) : jQ(document));    //IE是在子的iframe下面
                            doc.click(function (e) {
                                var target = jQ(e.target);
                                if (target.closest(".popWin").length <= 0
                                        && target.closest(".ui-datepicker").length <= 0
                                        && target.closest(".ui-widget-header").length <= 0) {
                                    dialog.cancel();
                                }
                            });
                        } finally {
                            initializing = false;
                        }
                    }, onFailure: function(transport) {
                        UI.alert("request search.jsp failed.");
                    }, onException: function(d, e) {
                        throw e;
                    }});
                }
            </script>
        </form>
    

    
