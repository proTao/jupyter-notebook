<script type="text/javascript"><!--
function needReloadNavOrNot(fid, count) {
    if(!fid) {
        return;
    }
    if (!count) {
        count = 0;
    }
    var navNewCount = "(0)";
    if ($("navNewCount_" + fid)) {
        navNewCount = $("navNewCount_" + fid).innerHTML;
    }
    var countString = "(" + count + ")";
    if (countString != navNewCount) {
        reloadNav();
    }
}

function playNotifyAudio() {
    var audioCtrl = $("audio_control");
    if (audioCtrl) {
        audioCtrl.innerHTML = '<embed src="" hidden="true" autostart="true"></embed>';
    }
}

function openExternalLink(aEl) {
    var url = aEl.href;
    var features = aEl.getAttribute("Features") || "";
    features = features.toLowerCase();
    var featuresArray = features.split(",");
    var featuresMap = {};
    for (var i = 0; i < featuresArray.length; i++) {
        var kvStr = featuresArray[i].trim();
        var idx = kvStr.indexOf("=");
        if (idx <= 0) {
            continue;
        }
        var key = kvStr.substring(0, idx).trim();
        var value = kvStr.substring(idx + 1).trim();
        if (key && value) {
            featuresMap[key] = value;
        }
    }
    if(!featuresMap.width) {
      featuresMap.width = 600;
    }
    if (!featuresMap.height) {
        featuresMap.height = 300;
    }
    if(!featuresMap.left) {
      featuresMap.left = (screen.width - featuresMap.width) / 2;
    }
    if (!featuresMap.top) {
        featuresMap.top = (screen.height - featuresMap.height) / 2;
    }

    featuresArray = [];
    for(var attr in featuresMap) {
      featuresArray.push(attr + "=" + featuresMap[attr]);
    }
    features = featuresArray.join(",");
    window.open(url, 'ExternalLinkWindow', features);
    return false;
}

function clickSharedMbox(index, postFix) {
    clicknode('cal_shr_det_nod_' + index, 'nav_shr_det_' + index);
    setOverflow();
}

function initCalURL(obj) {
    var activeNode = jQ("#nav_cal li.active");
    if (activeNode.length > 0) {
        var aHref= activeNode.find("a").attr("href");
        var currentHref = obj.href;
        currentHref = removeUrlParam(currentHref, "fid");
        var selectedFid = fGetParamValueByURL(aHref, "fid");
        obj.href = currentHref + "&fid=" + selectedFid;

    } else {
        jQ("#nav_cal").find("li").each(function() {
            jQ(this).find("a").attr('ignoreactive', true);
        });

    }
}

function reActiveFolderLink() {
    jQ("#nav_cal").find("li").each(function() {
        jQ(this).find("a").attr('ignoreactive', false);
    });
}
//--></script>


